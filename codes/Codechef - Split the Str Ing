#include<bits/stdc++.h>
#define MOD (ll)1000000007 
#define MOD2 998244353
#define ll long long int
#define ull unsigned long long int
#define lf double
#define ld long double
#define MAX LLONG_MAX
#define MIN LLONG_MIN 
#define UMAX ULLONG_MAX
#define pb push_back
#define endl "\n"  
using namespace std;

int main() 
{
	
	ll t;
	cin>>t;	
	while(t--)
	{
		ll n;
		cin>>n;
		string s;
		cin>>s;
		int flag=0;
		for(ll i=0;i<n-1;i++)
		{
			if(s[i]==s[n-1])
			{
				flag=1;
				break;
			}
		}
		if(flag==1)
			cout<<"YES\n";
		else
			cout<<"NO\n";
	}
	
	
	return 0;
}
